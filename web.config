<?xml version="1.0" encoding="utf-8"?>
<configuration>
<system.webServer>
    <handlers>
        <add name="Python FastCGI"
            path="*"
            verb="*"
            modules="FastCgiModule"
            scriptProcessor="C:\python\myweb\venv\Scripts\python.exe|C:\python\myweb\venv\Lib\site-packages\wfastcgi.py"
            resourceType="Unspecified"
            requireAccess="Script" />
    </handlers>
</system.webServer>
<appSettings>
    <add key="WSGI_HANDLER" value="run.app" />
    <add key="PYTHONPATH" value="C:\python\myweb" />
</appSettings>
</configuration>